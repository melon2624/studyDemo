<mapper namespace="User">
    <select id="selectOne" paramterType="com.zx.entity.User"
            resultType="com.zx.entity.User">
        select * from user where id = #{id} and username =#{username}
    </select>

    <select id="selectList" resultType="com.zx.entity.User">
        select * from user
    </select>
</mapper>